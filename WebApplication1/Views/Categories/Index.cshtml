@model IEnumerable<WebApplication1.Models.category>

@{
    ViewBag.Title = "Categories";
}


<h2 style="color:saddlebrown;">Categories</h2>


<p>
    <button>
    @Html.ActionLink("Create New Category", "Create")
        </button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.category1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.category1)
            </td>
            <td>
                <button>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
                    </button>
            </td>
        </tr>
    }

</table>